28248 silly resolved     version: '0.5.2',
28248 silly resolved     description: 'Traverse and transform objects by visiting every node on a recursive walk',
28248 silly resolved     author: { name: 'James Halliday' },
28248 silly resolved     license: 'MIT/X11',
28248 silly resolved     main: './index',
28248 silly resolved     repository:
